<%- include('../includes/header') %>
<div class="container">
  <div class="row">
    <table class="table table-hover table-bordered">
      <thead>
        <tr>
          <th>电影名字</th>
          <th>导演</th>
          <th>国家</th>
          <th>上映年份</th>
          <th>查看</th>
          <th>更新</th>
          <th>删除</th>
        </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < moive.length; i++) { %>
        <tr class="item-id-<%= moive[i]._id %>">
          <td><%= moive[i].title %></td>
          <td><%= moive[i].doctor %></td>
          <td><%= moive[i].country %></td>
          <td><%= moive[i].year %></td>
          <td><a class="btn btn-default" target="_blank" href="../moive/<%= moive[i]._id %>">查看</ a></td>
          <td><a class="btn btn-primary" target="_blank" href="../admin/update/<%= moive[i]._id %>">修改</ a></td>
          <td>
            <button class="btn btn-danger del" type="button" data-id="<%= moive[i]._id %>">删除</button>
          </td>
        </tr>
      <% } %>
      </tbody>
    </table>
  </div>
</div>
<%- include('../includes/footer') %>